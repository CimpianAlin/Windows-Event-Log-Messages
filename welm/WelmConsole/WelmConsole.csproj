  <PropertyGroup>
    <PostBuildEvent>setlocal

if "$(ConfigurationName)"=="Debug" goto end

if exist "$(SolutionDir)\dist" (
   rmdir /s /q "$(SolutionDir)\dist"
) 

set ILMERGE_PATH=$(SolutionDir)\packages\ILMerge.2.14.1208\tools\ilmerge.exe

if exist "%25ILMERGE_PATH%25" (
   mkdir "$(SolutionDir)\dist"

   start /wait "" "%25ILMERGE_PATH%25" $(TargetFileName) WelmLibrary.dll DocoptNet.dll NLog.dll CsvHelper.dll Newtonsoft.Json.dll /lib:"$(TargetDir)\"  /target:exe /out:"$(SolutionDir)\dist\welm.exe" /ndebug

   if exist "$(TargetDir)NLog.config" (
      copy "$(TargetDir)NLog.config" "$(SolutionDir)\dist\NLog.config" /y
   )

   if exist "$(SolutionDir)\Install-Features.ps1" (
      copy "$(SolutionDir)\Install-Features.ps1" "$(SolutionDir)\dist\Install-Features.ps1" /y
   )

   if exist "$(SolutionDir)\welm.bat" (
      copy "$(SolutionDir)\welm.bat" "$(SolutionDir)\dist\welm.bat" /y
   )
)

:end</PostBuildEvent>
